{% extends 'base.html.twig' %}

{% block body %}
    Ma Ville
{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h1>Nom de la ville : {{ ville.nom }}</h1>
        <ol>
            {% for ecole in ville.ecoles %}
            <li>
                {{ ecole.nom }} : {{ ecole.adresse }} : {{ ecole.etudiant.nom }}
            </li>
            {% endfor %}
        </ol>
    </div>
{% endblock %}
